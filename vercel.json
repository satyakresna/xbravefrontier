{
    "github": {
        "enabled": false,
        "silent": true
    },
    "builds": [
      {
        "src": "api/v1/**",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "/assets/**",
        "use": "@vercel/static"
      },
      {
        "src": "/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/dbbs",
        "dest": "api/v1/dbbs/index.js",
        "headers": {
            "Cache-Control": "public, s-maxage=60, stale-while-revalidate",
            "Access-Control-Allow-Origin": "*"
        }
      },
      {
        "src": "/api/v1/omniunits",
        "dest": "api/v1/omniunits/index.js",
        "headers": {
            "Cache-Control": "public, s-maxage=60, stale-while-revalidate",
            "Access-Control-Allow-Origin": "*"
        }
      },
      {
        "src": "/api/v1/omniunits/(?<name>.*)",
        "dest": "api/v1/omniunits/[name].js",
        "headers": {
            "Cache-Control": "public, s-maxage=60, stale-while-revalidate",
            "Access-Control-Allow-Origin": "*"
        }
      },
      { "handle": "filesystem" },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/dist/(.*)",
        "dest": "/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
}